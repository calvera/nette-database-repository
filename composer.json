{
    "name": "efabrica/nette-repository",
    "type": "library",
    "description": "Repositories for Nette database",
    "keywords": [
        "php",
        "nette",
        "database",
        "repository"
    ],
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Popelis",
            "email": "richard@popelis.sk"
        },
        {
            "name": "Martin Petricko",
            "email": "matopetricko@gmail.com"
        },
        {
            "name": "Samuel Kelemen",
            "email": "kelemen.samuel@gmail.com"
        },
        {
            "name": "Michal Lulco",
            "email": "michal.lulco@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.efabrica.sk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "nette/database": "^3.1",
        "nette/di": "^3.1",
        "nette/caching": "^3.1",
        "symfony/polyfill-php80": "^1.27",
        "efabrica/iris-client": "^8.6",
        "nette/php-generator": "^3.6",
        "symfony/console": "^5.4",
        "doctrine/inflector": "^2.0",
        "nikic/php-parser": "^4.16"
    },
    "require-dev": {
        "nette/bootstrap": "^3.1",
        "tracy/tracy": "^2.9",
        "nette/http": "^3.2",
        "phpunit/phpunit": "^9.6",
        "nette/application": "^3.1",
        "nesbot/carbon": "^2.68"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\NetteRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
